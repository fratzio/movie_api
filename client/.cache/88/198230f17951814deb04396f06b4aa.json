,"column":22},"source":"components/login-view/login-view.jsx","original":{"line":72,"column":22}},{"name":"logInFunc","generated":{"line":98,"column":0},"source":"components/login-view/login-view.jsx","original":{"line":73,"column":2}},{"name":"logInFunc","generated":{"line":98,"column":2},"source":"components/login-view/login-view.jsx","original":{"line":73,"column":2}},{"generated":{"line":98,"column":11},"source":"components/login-view/login-view.jsx","original":{"line":73,"column":11}},{"name":"PropTypes","generated":{"line":98,"column":13},"source":"components/login-view/login-view.jsx","original":{"line":73,"column":13}},{"name":"func","generated":{"line":98,"column":32},"source":"components/login-view/login-view.jsx","original":{"line":73,"column":23}},{"generated":{"line":98,"column":36},"source":"components/login-view/login-view.jsx","original":{"line":73,"column":13}},{"name":"isRequired","generated":{"line":98,"column":37},"source":"components/login-view/login-view.jsx","original":{"line":73,"column":28}},{"generated":{"line":99,"column":0},"source":"components/login-view/login-view.jsx","original":{"line":72,"column":22}},{"generated":{"line":99,"column":1},"source":"components/login-view/login-view.jsx","original":{"line":72,"column":0}}],"sources":{"components/login-view/login-view.jsx":"import React, { useState } from 'react';\nimport Form from 'react-bootstrap/Form';\nimport Button from 'react-bootstrap/Button';\nimport Container from 'react-bootstrap/Container';\nimport axios from 'axios';\nimport './login-view.scss';\nimport PropTypes from 'prop-types';\n\nexport function LoginView(props) {\n  // Calling useState() method with empty string (= initial value of login variable)\n  const [username, setUsername] = useState('');\n  const [password, setPassword] = useState('');\n\n  const handleSubmit = (e) => {\n    // prevents the default refresh after submit button has been clicked\n    e.preventDefault();\n    // console.log(username, password);\n\n    // handles form validation\n\n    /* Send a request to the server for authentication */\n    axios\n      .post('https://vfa.herokuapp.com/login', {\n        Username: username,\n        Password: password,\n      })\n      .then((response) => {\n        const data = response.data;\n        // console.log(data);\n        // Send data to prop\n        props.logInFunc(data);\n      })\n      .catch(() => {\n        return alert('Invalid username or password. Please try again');\n      });\n  };\n\n  return (\n    <Container>\n      <h1>Log in</h1>\n      <Form className=\"login-form\">\n        <Form.Group controlId=\"formBasicUsername\">\n          <Form.Label>Username:</Form.Label>\n          <Form.Control\n            type=\"text\"\n            value={username}\n            placeholder=\"Enter username\"\n            onChange={(e) => setUsername(e.target.value)}\n          />\n        </Form.Group>\n        <Form.Group>\n          <Form.Label>Password:</Form.Label>\n          <Form.Control\n            type=\"password\"\n            value={password}\n            placeholder=\"Enter password\"\n            onChange={(e) => setPassword(e.target.value)}\n          />\n        </Form.Group>\n        <Button\n          variant=\"btn-lg btn-dark btn-block\"\n          type=\"submit\"\n          onClick={handleSubmit}\n        >\n          Log in\n        </Button>\n      </Form>\n    </Container>\n  );\n}\n\nLoginView.propTypes = {\n  logInFunc: PropTypes.func.isRequired,\n};\n"},"lineCount":null}},"error":null,"hash":"d15dcdede12ca2f5c194d8df51b29e6e","cacheData":{"env":{}}}